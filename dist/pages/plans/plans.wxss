.plans .plans-top {
  line-height: 88rpx;
  display: flex;
  align-items: center;
  padding: 0 24rpx;
  box-shadow: 0rpx 0rpx 10rpx 5rpx #fff5f0;
}
.plans .plans-top .address {
  position: relative;
  text-align: left;
  width: 240rpx;
  height: 88rpx;
}
.plans .plans-top .address .showcitys {
  color: #999;
  font-size: 30rpx;
}
.plans .plans-top .address .pink {
  color: #ffc4a6;
}
.plans .plans-top .address .s-i {
  display: inline-block;
  margin: 0 5rpx;
  transform: translateY(-2rpx);
}
.plans .plans-top .time {
  text-align: center;
  flex: 1;
  font-size: 36rpx;
  color: #ffc4a6;
}
.plans .plans-top .time text {
  font-size: 28rpx;
}
.plans .plans-top .rank {
  text-align: right;
  width: 200rpx;
  height: 100%;
  color: #999;
  font-size: 30rpx;
}
.plans .plans-top .rank text {
  display: inline-block;
  transform: rotate(180deg);
}
.plans .plans-top .pinks {
  color: #ffc4a6;
}
.plans .plans-top .as {
  width: 160rpx;
  font-size: 30rpx;
  border-bottom: 1rpx solid #ccc;
  border-top: 1rpx solid #ccc;
  text-align: center;
  color: #333;
  line-height: 80rpx;
  background-color: #fff;
}
.plans .plans-top .as:nth-last-child(1) {
  border-top: none;
}
.plans .plans-top .asa {
  color: #ffc4a6;
}
.plans .plans-content-wrap {
  padding: 0 24rpx;
}
.plans .plans-content-wrap .plans-content {
  position: relative;
  border-bottom: 1rpx solid #ccc;
  margin-top: 19rpx;
}
.plans .plans-content-wrap .plans-content image {
  width: 690rpx;
  height: 518rpx;
}
.plans .plans-content-wrap .plans-content .p-c-title {
  font-size: 30rpx;
  color: #333;
  font-weight: 700;
  line-height: 44rpx;
}
.plans .plans-content-wrap .plans-content .p-c-bottom {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 30rpx;
  margin-top: 18rpx;
}
.plans .plans-content-wrap .plans-content .p-c-bottom .p-c-b-left {
  line-height: 36rpx;
  font-size: 24rpx;
  color: #999;
  background-color: #e6e6e6;
  padding: 0 6rpx;
  position: relative;
  transform: translate(8rpx, 0);
  border-radius: 5rpx;
}
.plans .plans-content-wrap .plans-content .p-c-bottom .p-c-b-left::before {
  content: '';
  position: absolute;
  top: 50%;
  left: 0;
  border: 14rpx solid #e6e6e6;
  border-color: transparent transparent #e6e6e6 #e6e6e6;
  transform: translate(-12rpx, -50%) rotate(45deg);
}
.plans .plans-content-wrap .plans-content .p-c-bottom .p-c-b-left::after {
  content: '';
  position: absolute;
  left: 0;
  top: 50%;
  width: 6rpx;
  height: 6rpx;
  background: #fff;
  transform: translate(-8rpx, -50%);
}
.plans .plans-content-wrap .plans-content .p-c-bottom .p-c-b-right {
  font-size: 32rpx;
  color: #ff4d4d;
}
.plans .setheight {
  height: 1100rpx;
  overflow: hidden;
}
.city-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 88rpx;
  left: 0;
  z-index: 999;
  background-color: rgba(0, 0, 0, 0.6);
}
.citys {
  position: absolute;
  top: 0rpx;
  left: 0;
  display: flex;
}
.citys .sv-wrap {
  height: 312rpx;
  width: 220rpx;
  overflow: hidden;
}
.citys .sv {
  margin-top: -1rpx;
  border-top: 1rpx solid #ccc;
}
.citys .city-one,
.citys .city-two {
  font-size: 30rpx;
  color: #333;
  border-top: 1rpx solid #ccc;
  border-bottom: 1rpx solid #ccc;
  margin-top: -1rpx;
  line-height: 78rpx;
  width: 220rpx;
  background-color: #fff;
  padding-left: 26rpx;
}
.citys .city-two {
  background-color: #eee;
}
.citys .current {
  background-color: #f5f5f5;
  color: #ffc4a6;
}
.c-m-out {
  position: absolute;
  top: 0;
  right: 0;
}
page {
  height: 100%;
}
.nogoods::after {
  content: '已售罄';
  position: absolute;
  font-size: 30rpx;
  color: #fff;
  top: 0;
  right: 12rpx;
  padding: 14rpx 20rpx;
  background-color: rgba(0, 0, 0, 0.3);
}
.nolist {
  padding-top: 20rpx;
  border-top: 1rpx solid #ccc;
  width: 100%;
  text-align: center;
  font-size: 26rpx;
  font-weight: 700;
  color: #ffc4a6;
}
.nolist text {
  transform: translateY(-90rpx);
}
.ssss {
  height: 100%;
  border-top: none;
  display: flex;
  align-items: center;
  justify-content: center;
}
